@model IEnumerable<FinalA.Models.Division>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>NHL Division</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
  
            @Html.ActionLink(item.Name, "details", new { id = item.DivisionID })
        </td>
        <td>
           
        
        </td>
    </tr>
}

</table>
